
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NoticeManage</title>
    <link href="~/CSS/base.css" rel="stylesheet" />
    <link href="~/CSS/Notice/NoticeManage.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/js/base.js"></script>
    <link href="~/Scripts/kindeditor-4.1.7/themes/default/default.css" rel="stylesheet" />
    <link href="~/Scripts/kindeditor-4.1.7/plugins/code/prettify.css" rel="stylesheet" />
    <script src="~/Scripts/kindeditor-4.1.7/kindeditor-min.js"></script>
    <script src="~/Scripts/kindeditor-4.1.7/lang/zh_CN.js"></script>
    <script src="~/Scripts/kindeditor-4.1.7/plugins/code/code.js"></script>

    <script src="~/Scripts/js/Notice/NoticeManage.js"></script>
</head>
<body>
    <form>
        @*<div class="top">
            <div class="top-main">
                网校平台系统
            </div>
        </div>
        <div class="menu">
        </div>*@
        <div class="main">
            <div class="main-div NoticeOperation">
                <p class="title"><span>添加通知公告</span></p>
                <div class="content">
                    <p class="pTitle">请填写标题：</p>
                    <input type="text" class="textBox txtTitle" />
                    <p class="pContent">请填写内容：</p>
                    <textarea id="noticeContent" name="noticeContent" class="noticeContent"></textarea>
                </div>
                <div class="btnDiv">
                    <input type="button" class="btn btn-BlueBorder OKBtn" name="OKBtn" value="确定" />
                    <input type="button" class="btn btn-BlueBorder cancelBtn" name="cancelBtn" value="取消" />
                </div>
            </div>
        </div>
        @*<div class="footer">
            footer
        </div>*@
        <input type="hidden" id="hdnID" value="@Html.ViewBag.id"></form>
</body>
</html>
