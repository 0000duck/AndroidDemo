@using HYZK.FrameWork.Common
@using HYZK.FrameWork.Web.Controls
@using HYZK.Account.Contract
@using HYZK.FrameWork.Utility

@model DKLManager.Contract.Model.DeviceOrderEntifyModel
@{
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
    <style type="text/css">
    </style>
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">
        $(document).ready(function () {
            $("#BirthDate").datepicker(
                {
                    changeYear: true,
                    changeMonth: true,
                });
        });
    </script>
   <script type="text/javascript">
       $(document).ready(function () {
           $("#BirthDate1").datepicker(
            {
                changeYear: true,
                changeMonth: true,
            });
       });



       
</script>

}
@section MainContent{
    <div class="portlet-body form-horizontal form-bordered form-row-stripped">
    <div class="row-fluid">




        <div class="control-group">
            <label class="control-label">设备数量：</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.Count, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.Count)</span>
            </div>
        </div>


        <div class="control-group">
            <label class="control-label">开始时间：</label>
            <div class="controls">
                @Html.TextBox("BirthDate", Model.StartTime.ToCnDataString(), new { @class = "m-wrap" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.StartTime)</span>
            </div>
        </div>


        <div class="control-group">
            <label class="control-label">状态：</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.OrderState, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.OrderState)</span>
            </div>
        </div>

    </div>
</div>
}