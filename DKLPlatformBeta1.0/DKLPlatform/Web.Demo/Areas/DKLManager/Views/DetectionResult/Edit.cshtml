@using HYZK.FrameWork.Common
@using HYZK.FrameWork.Web.Controls
@using HYZK.Account.Contract
@using HYZK.FrameWork.Utility

@model DKLManager.Contract.Model.DetectionResult
@{
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
    <style type="text/css">
    </style>
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">
        $(document).ready(function () {
            $("#BirthDate").datepicker(
                {
                    changeYear: true,
                    changeMonth: true,
                });
        });
    </script>

}
@section MainContent{
    <div class="portlet-body form-horizontal form-bordered form-row-stripped">
        <div class="row-fluid">

            <div class="control-group">
                <label class="control-label">选择：</label>
                <div class="controls">
                    @Html.DropDownList("Physicochemical")
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Physicochemical)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">车间：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Workshop, new { @class = "m-wrap small" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Workshop)</span>
                </div>
            </div>


            <div class="control-group">
                <label class="control-label">岗位：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Post, new { @class = "m-wrap small" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Post)</span>
                </div>
            </div>


        </div>
    </div>
}








