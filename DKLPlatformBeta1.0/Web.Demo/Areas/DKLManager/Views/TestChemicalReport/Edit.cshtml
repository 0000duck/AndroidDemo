@using HYZK.FrameWork.Common
@using HYZK.FrameWork.Web.Controls
@using HYZK.Account.Contract
@using HYZK.FrameWork.Utility

@model DKLManager.Contract.Model.TestChemicalReport
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
    <style type="text/css">
    </style>
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">

    </script>


    <script type="text/javascript">
        $("#ProjectClosingDate").datepicker(
            {
                changeYear: true,
                changeMonth: true,
            });
    </script>
}
@section MainContent{

    <div class="portlet box red m10">
        <div class="control-group">
            <label class="control-label">项目名称</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.ProjectName, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.ProjectName)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">采样项目</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.SampleProject, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.SampleProject)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">样品编号</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.SampleNumber, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.SampleNumber)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">车间</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.WorkShop, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.WorkShop)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">职位</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.Job, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.Job)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">测量地点</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.Location, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.Location)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">影响因素</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.Factor, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.Factor)</span>
            </div>
        </div>

        <div class="control-group">
            <label class="control-label">CSTEL</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.CSTEL, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.CSTEL)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">CTWA</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.CTWA, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.CTWA)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">CMAC</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.CMAC, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.CMAC)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">超限倍数</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.Multiple, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.Multiple)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">结果判定</label>
            <div class="controls">
                @Html.TextBoxFor(m => m.ResultVerdict, new { @class = "m-wrap small" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.ResultVerdict)</span>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">时间</label>
            <div class="controls">
                @Html.TextBox("ProjectClosingDate", Model.CreateTime, new { @class = "m-wrap" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.CreateTime)</span>
            </div>
        </div>
    </div>


}
