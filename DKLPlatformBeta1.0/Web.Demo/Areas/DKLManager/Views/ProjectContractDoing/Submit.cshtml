                </div>
                <div class="control-group1">
                    <label class="control-label">乙方地址：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.AddressB, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">乙方邮箱：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.EmailB, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">乙方邮编：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.ZipCodeB, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">乙方授权人姓名：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.AuthorizedPersonB, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">乙方授权人职务：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.AuthorizedPersonJobB, new { @class = "m-wrap small" })

                    </div>
                </div>



            </div>

        </div>

}
