                        @Html.TextBoxFor(m => m.RepresentativeA, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">甲方法定代表人电话：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.RepresentativeATel, new { @class = "m-wrap small" })

                    </div>
