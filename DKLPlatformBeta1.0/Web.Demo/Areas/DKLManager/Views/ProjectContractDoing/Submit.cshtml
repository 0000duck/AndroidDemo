                        @Html.TextBoxFor(m => m.FaxA, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">甲方联系人：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.ContactPersonA, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">甲方电话：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.TelA, new { @class = "m-wrap small" })

                    </div>
                </div>
