                    </div>
                </div>


                <div class="control-group1">
                    <label class="control-label">乙方法定代表人：</label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.RepresentativeB, new { @class = "m-wrap small" })

                    </div>
                </div>
                <div class="control-group1">
                    <label class="control-label">乙方法定代表人电话：</label>
                    <div class="controls">
